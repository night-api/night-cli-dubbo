<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	xsi:schemaLocation="http://www.springframework.org/schema/beans						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"	default-lazy-init="false">	<!-- 缓存配置 -->	<bean name="cacheService"		class="com.kaistart.gateway.common.cache.impl.CacheServiceImpl">		<property name="host" value="${redis.host}" />		<property name="port" value="${redis.port}" />		<property name="password" value="${redis.password}" />		<property name="timeout" value="${redis.timeout}" />	</bean>		<!-- zk服务配置 -->	<bean name="zkService"		class="com.kaistart.gateway.common.tool.ZkService">		<property name="zkUrl" value="${zk.url}" />		<property name="gatewayMgrService" ref="gatewayMgrService" />	</bean>		<!-- 网关业务流程中的核心配置 -->	<bean name="runConfig"		class=" com.kaistart.gateway.config.RunConfig">		<property name="blackUserAgent" value="${blackUserAgent}" />		<property name="blackIp" value="${blackIp}" />		<property name="userTokenExpiredTime" value="${userTokenExpiredTime}" />		<property name="isMock" value="${isMock}" />		<property name="processor" value="${processor}" />		<property name="online" value="${gateway.online}" />		<property name="applicationName" value="${applicationName}" />		<property name="zookeeperAddress" value="${zookeeperAddress}" />	</bean>	</beans>

